{
  "name": "my-personal-blog",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b",
    "build:production": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006 --no-version-updates",
    "build-storybook": "storybook build",
    "lint-staged": "lint-staged",
    "ts-check": "tsc -b ./tsconfig.json",
    "steiger": "steiger ./src",
    "check-all": "npm run ts-check && npm run lint && npm run steiger",
    "generate-graphql": "graphql-codegen --config apollo-codegen-config.ts",
    "prettify": "eslint --cache --fix src && prettier --cache --write src",
    "plop": "plop input --destinationpath $INIT_CWD",
    "gen-db-types": "npx supabase gen types --lang=typescript --local --schema public > ./src/shared/api/supabase-client/database.types.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run prettify"
    ]
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@hookform/resolvers": "^3.10.0",
    "@supabase/supabase-js": "^2.48.0",
    "@tanstack/react-query": "^5.64.2",
    "framer-motion": "^11.3.28",
    "jotai": "^2.9.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/addon-onboarding": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/react-vite": "^8.2.9",
    "@types/node": "^22.4.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-boundaries": "^4.2.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-unused-imports": "^4.1.3",
    "eslint-plugin-vitest": "^0.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.1",
    "plop": "^4.0.1",
    "prettier": "^3.3.3",
    "steiger": "^0.4.0",
    "storybook": "^8.2.9",
    "supabase": "^2.6.8",
    "typescript": "^5.5.4",
    "vite": "^5.4.1",
    "vitest": "^2.0.5"
  }
}
